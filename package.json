{
  "name": "authing-js-sdk",
  "version": "3.0.0",
  "description": "Official sdk of Authing",
  "main": "dist/authing-js-sdk.js",
  "browser": "dist/authing-js-sdk-browser.min.js",
  "unpkg": "dist/authing-js-sdk-browser.min.js",
  "prepublishOnly": "npm run build",
  "scripts": {
    "lint": "eslint --fix .",
    "test": "nyc ava -vs",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov | codecov",
    "start": "nyc --reporter html ava -vs && open ./.nyc_output/index.html",
    "build": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Authing/authing-js-sdk.git"
  },
  "keywords": [
    "authing",
    "auth",
    "oauth"
  ],
  "author": "authing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Authing/authing-js-sdk/issues"
  },
  "homepage": "https://github.com/Authing/authing-js-sdk#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "jsencrypt": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!**/_*/*.js",
      "!**/_*.js"
    ]
  },
  "nyc": {
    "lines": 10,
    "check-coverage": true,
    "reporter": [
      "text-lcov"
    ],
    "report-dir": "./.nyc_output",
    "exclude": [
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**"
    ]
  }
}
